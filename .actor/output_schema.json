{
    "actorOutputSchemaVersion": 1,
    "title": "Domain.com.au Property Data",
    "description": "Comprehensive Australian real estate property data including prices, features, and location details",
    "properties": {
        "id": {
            "type": "string",
            "title": "Property ID",
            "description": "Unique identifier for the property listing",
            "template": "{{value}}"
        },
        "url": {
            "type": "string",
            "title": "Property URL",
            "description": "Full URL to the property listing on Domain.com.au",
            "template": "{{value}}"
        },
        "address": {
            "type": "string",
            "title": "Address",
            "description": "Complete street address of the property",
            "template": "{{value}}"
        },
        "suburb": {
            "type": "string",
            "title": "Suburb",
            "description": "Suburb/locality name",
            "template": "{{value}}"
        },
        "state": {
            "type": "string",
            "title": "State",
            "description": "Australian state (NSW, VIC, QLD, etc.)",
            "template": "{{value}}"
        },
        "postcode": {
            "type": "string",
            "title": "Postcode",
            "description": "Australian postcode",
            "template": "{{value}}"
        },
        "price": {
            "type": "string",
            "title": "Price",
            "description": "Property price (may be range, fixed price, or text like 'Contact Agent')",
            "template": "{{value}}"
        },
        "propertyType": {
            "type": "string",
            "title": "Property Type",
            "description": "Type of property (House, Apartment, Townhouse, etc.)",
            "template": "{{value}}"
        },
        "beds": {
            "type": "string",
            "title": "Bedrooms",
            "description": "Number of bedrooms",
            "template": "{{value}}"
        },
        "baths": {
            "type": "string",
            "title": "Bathrooms",
            "description": "Number of bathrooms",
            "template": "{{value}}"
        },
        "parking": {
            "type": "string",
            "title": "Parking Spaces",
            "description": "Number of parking spaces",
            "template": "{{value}}"
        },
        "landSize": {
            "type": "string",
            "title": "Land Size",
            "description": "Size of land in square meters",
            "template": "{{value}}"
        },
        "floorSize": {
            "type": "string",
            "title": "Floor Size",
            "description": "Internal floor size in square meters",
            "template": "{{value}}"
        },
        "description": {
            "type": "string",
            "title": "Description",
            "description": "Full property description",
            "template": "{{value}}"
        },
        "agent": {
            "type": "string",
            "title": "Agent Name",
            "description": "Name of the listing agent",
            "template": "{{value}}"
        },
        "agency": {
            "type": "string",
            "title": "Agency Name",
            "description": "Name of the real estate agency",
            "template": "{{value}}"
        },
        "imageUrl": {
            "type": "string",
            "title": "Primary Image URL",
            "description": "URL of the main property image",
            "template": "{{value}}"
        },
        "images": {
            "type": "string",
            "title": "All Images",
            "description": "Array of all property image URLs (JSON string)",
            "template": "{{#each (jsonParse value)}}<img src=\"{{this}}\" style=\"max-width:100px;max-height:100px;margin:2px;\"/>{{/each}}"
        },
        "latitude": {
            "type": "string",
            "title": "Latitude",
            "description": "Geographic latitude coordinate",
            "template": "{{value}}"
        },
        "longitude": {
            "type": "string",
            "title": "Longitude",
            "description": "Geographic longitude coordinate",
            "template": "{{value}}"
        },
        "inspectionTimes": {
            "type": "string",
            "title": "Inspection Times",
            "description": "Scheduled inspection times for the property (JSON string)",
            "template": "{{#each (jsonParse value)}}{{this}}<br/>{{/each}}"
        },
        "features": {
            "type": "string",
            "title": "Property Features",
            "description": "List of property features and amenities (JSON string)",
            "template": "{{#each (jsonParse value)}}â€¢ {{this}}<br/>{{/each}}"
        },
        "isNew": {
            "type": "string",
            "title": "New Listing",
            "description": "Whether this is a newly listed property (true/false)",
            "template": "{{#if (eq value 'true')}}âœ… New{{else}}ðŸ“… Existing{{/if}}"
        },
        "inspectionTime": {
            "type": "string",
            "title": "Next Inspection",
            "description": "Next scheduled inspection time",
            "template": "{{value}}"
        },
        "scrapedAt": {
            "type": "string",
            "title": "Scraped At",
            "description": "ISO timestamp when the data was collected",
            "template": "{{value}}"
        },
        "source": {
            "type": "string",
            "title": "Data Source",
            "description": "Source of the data (domain.com.au)",
            "template": "{{value}}"
        }
    }
}